{
  "name": "ello-webapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -f public/index.html public/static/*.js public/static/*.css",
    "build": "node node_modules/webpack/bin/webpack.js -p --colors --progress --config=webpack.prod.config.js",
    "dev": "node_modules/jade/bin/jade.js dev/dev.jade --pretty && node dev/dev-server.js",
    "docs:build": "npm run docs:clean && gitbook build",
    "docs:clean": "rimraf _book",
    "docs:serve": "npm run docs:clean && gitbook serve",
    "lint": "eslint src test",
    "spec": "NODE_ENV=test mocha --compilers js:babel/register test/*.js test/**/*.js test/**/**/*.js",
    "start": "node server.js",
    "postinstall": "npm run build",
    "test": "npm run lint && NODE_ENV=test npm run spec"
  },
  "engines": {
    "node": "4.1.1"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.0.0",
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "babel-eslint": "*",
    "babel-loader": "^5.3.2",
    "classnames": "^2.2.0",
    "css-loader": "^0.21.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "history": "^1.13.0",
    "html-webpack-plugin": "^1.6.2",
    "humps": "^1.0.0",
    "isomorphic-fetch": "^2.2.0",
    "jade": "^1.11.0",
    "localforage": "^1.3.0",
    "lodash.uniq": "^3.2.2",
    "mousetrap": "^1.5.3",
    "newrelic": "^1.23.0",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.4.1",
    "node-static": "^0.7.7",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-persist": "^1.4.0",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^1.1.0",
    "chai": "^3.4.0",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "*",
    "eslint-plugin-react": "^3.6.3",
    "gitbook-cli": "^1.0.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "sinon": "^1.17.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  }
}
