{
  "name": "ello-webapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean" : "rm -f public/index.html public/assets/*.js public/assets/*.css",
    "postinstall": "npm run build",
    "build": "node node_modules/webpack/bin/webpack.js -p --colors --progress --config=webpack.prod.config.js",
    "dev": "node dev/dev-server.js",
    "docs:build": "npm run docs:clean && gitbook build",
    "docs:clean": "rimraf _book",
    "docs:serve": "npm run docs:clean && gitbook serve",
    "lint": "eslint src test",
    "spec": "mocha --compilers js:babel/register test/*.js test/**/*.js test/**/**/*.js",
    "start": "node server.js",
    "test": "npm run lint && npm run spec"
  },
  "engines": {
    "node": "0.12.7"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.0.0",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "*",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.2",
    "classnames": "^2.1.3",
    "css-loader": "^0.17.0",
    "error-stack-parser": "^1.2.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "humps": "^0.6.0",
    "isomorphic-fetch": "^2.1.1",
    "mousetrap": "^1.5.3",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.3.0",
    "node-static": "^0.7.7",
    "react": "^0.13.3",
    "react-redux": "^2.1.1",
    "react-router": "=1.0.0-beta3",
    "react-transform-catch-errors": "^0.1.1",
    "react-transform-webpack-hmr": "^0.1.3",
    "redux": "^2.0.0",
    "redux-logger": "^1.0.0",
    "redux-thunk": "^0.1.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-react": "*",
    "gitbook-cli": "^0.3.6",
    "jsdom": "^3.1.2",
    "mocha": "^2.3.2",
    "mocha-jsdom": "^1.0.0",
    "sinon": "^1.16.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  }
}
