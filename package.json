{
  "name": "ello-webapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -f public/index.html public/static/*.js public/static/*.css",
    "build": "npm run build:web && npm run build:server",
    "build:server": "NODE_ENV=production node node_modules/webpack/bin/webpack.js -p --config=webpack.server.config.js",
    "build:web": "NODE_ENV=production node node_modules/webpack/bin/webpack.js -p --config=webpack.prod.config.js",
    "dev": "npm run clean && node_modules/jade/bin/jade.js dev/dev.jade --pretty && DEBUG=express:* NODE_ENV=development babel-node dev/dev-server.js",
    "lint": "eslint src test",
    "spec": "NODE_ENV=test mocha --compilers js:babel-core/register test/*.js test/**/*.js test/**/**/*.js",
    "start": "NODE_ENV=production node dist/server-iso.js",
    "start:debug": "DEBUG=express:* NODE_ENV=production node dist/server-iso.js",
    "start:old": "NODE_ENV=production babel-node server.js",
    "postinstall": "npm run build",
    "test": "npm run lint && NODE_ENV=test npm run spec"
  },
  "engines": {
    "node": "5.10.1",
    "npm": "3.8.3"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "autoprefixer-loader": "3.2.0",
    "babel": "6.5.2",
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-lodash": "2.2.1",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "classnames": "2.2.3",
    "css-loader": "0.23.1",
    "dotenv": "2.0.0",
    "estraverse-fb": "1.3.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.15.0",
    "humps": "1.0.0",
    "isomorphic-fetch": "2.2.1",
    "jade": "1.11.0",
    "librato-node": "4.1.0",
    "lodash": "4.8.1",
    "morgan": "1.7.0",
    "mousetrap": "1.5.3",
    "newrelic": "1.26.1",
    "node-libs-browser": "1.0.0",
    "node-sass": "3.4.2",
    "node-static": "0.7.7",
    "react": "15.0.0-rc.2",
    "react-dom": "15.0.0-rc.2",
    "react-helmet": "3.0.1",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-router-redux": "4.0.1",
    "redux": "3.3.1",
    "redux-logger": "2.6.1",
    "redux-persist": "2.0.1",
    "redux-thunk": "2.0.1",
    "sass-loader": "3.2.0",
    "simple-oauth2": "0.5.1",
    "style-loader": "0.13.1",
    "throng": "3.0.0",
    "trunc-html": "1.0.2",
    "webpack": "1.12.14",
    "webpack-sources": "0.1.1"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "2.0.2",
    "chai": "3.5.0",
    "eslint": "2.6.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.2.3",
    "jsdom": "8.3.0",
    "mocha": "2.4.5",
    "mocha-jsdom": "1.1.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.2",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "source-map-support": "0.4.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
