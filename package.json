{
  "name": "ello-webapp",
  "version": "3.0.20",
  "private": true,
  "scripts": {
    "clean": "rm -f public/index.html public/static/*.js public/static/*.map public/static/*.css",
    "build": "npm run build:web && npm run build:server",
    "build:server": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --config=webpack.server.config.js",
    "build:web": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js -p --config=webpack.prod.config.js",
    "dev": "npm run clean && DEBUG=express:* NODE_ENV=development babel-node server-dev.js",
    "lint": "eslint src test",
    "start": "NODE_ENV=production node dist/server-iso-entrypoint.js",
    "start:debug": "DEBUG=express:* NODE_ENV=production node dist/server-iso-entrypoint.js",
    "start:old": "NODE_ENV=production babel-node server.js",
    "postinstall": "npm run build",
    "pretest": "npm run lint",
    "test": "npm run test:unit && npm run test:server && npm run test:karma",
    "test:cov": "NODE_ENV=test babel-node ./node_modules/isparta/bin/isparta cover --report text ./node_modules/mocha/bin/_mocha -- --recursive",
    "test:karma": "NODE_ENV=test karma start --single-run --no-auto-watch",
    "test:karma:watch": "NODE_ENV=test karma start",
    "test:server": "NODE_ENV=test mocha test/server --recursive",
    "test:unit": "NODE_ENV=test mocha test/unit --recursive",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "engines": {
    "node": "5.10.1",
    "npm": "3.8.3"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "autoprefixer-loader": "3.2.0",
    "babel": "6.5.2",
    "babel-cli": "6.9.0",
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-lodash": "3.1.5",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "dotenv": "2.0.0",
    "estraverse-fb": "1.3.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "honeybadger": "1.1.0",
    "honeybadger-js": "0.4.2",
    "html-webpack-plugin": "2.20.0",
    "humps": "1.1.0",
    "isomorphic-fetch": "2.2.1",
    "jwt-decode": "2.0.1",
    "librato-node": "4.1.0",
    "lodash": "4.13.1",
    "morgan": "1.7.0",
    "mousetrap": "1.6.0",
    "newrelic": "1.28.0",
    "node-libs-browser": "1.0.0",
    "node-sass": "3.7.0",
    "node-static": "0.7.7",
    "pm2": "1.1.3",
    "react": "15.0.0",
    "react-addons-shallow-compare": "15.0.0",
    "react-dom": "15.0.0",
    "react-helmet": "3.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-persist": "3.2.2",
    "redux-saga": "0.10.5",
    "reselect": "2.5.1",
    "sass-loader": "3.2.0",
    "simple-oauth2": "0.7.0",
    "style-loader": "0.13.1",
    "trunc-html": "1.0.2",
    "webpack": "1.13.1",
    "webpack-sources": "0.1.2"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "2.0.2",
    "chai": "3.5.0",
    "chai-generator": "2.0.3",
    "chai-http": "3.0.0",
    "enzyme": "2.3.0",
    "eslint": "2.12.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.4.0",
    "eslint-plugin-react": "5.1.1",
    "fetch-pretender": "1.5.0",
    "isparta": "4.0.0",
    "jsdom": "9.2.1",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-mocha": "1.0.1",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "nock": "8.0.0",
    "phantomjs-prebuilt": "2.1.7",
    "react-addons-test-utils": "15.0.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.6",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "source-map-support": "0.4.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
